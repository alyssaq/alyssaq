#header
  background: color-header-background
  padding-bottom: nav-height
  position: relative

#header-outer
  height: 100%
  position: relative

#header-inner
  position: relative

#header-title
  clearfix()
  text-align: center
  padding: 30px 30px
  .logo-wrap, .subtitle-wrap
    float: left
  .subtitle-wrap
    padding: 10px 0px 0px
    margin-left: 20px
  .subtitle
    font-size: 20px
    line-height: logo-height + 20
    letter-spacing: 0.03em
    color: color-nav-foreground
